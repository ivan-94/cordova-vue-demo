{
  "name": "cordova-vue-demo",
  "version": "1.0.0",
  "description": "Example Vue1.x Cordova App with Babel, webpack and Hot Reloading",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config ./build/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.prod.js",
    "build-test": "cross-env NODE_ENV=test-build webpack --config ./build/webpack.config.prod.js",
    "watch": "cross-env NODE_ENV=development webpack --config ./build/webpack.config.dev.js --progress --watch",
    "ios": "cordova run ios",
    "android": "cordova run android",
    "prepare": "cordova prepare",
    "compile": "cordova build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carney520/cordova-vue-demo.git"
  },
  "keywords": [
    "vue",
    "cordova",
    "babel",
    "webpack"
  ],
  "author": "ivan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carney520/cordova-vue-demo/issues"
  },
  "homepage": "https://github.com/carney520/cordova-vue-demo#readme",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-runtime": "^6.18.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-html": "^1.5.5",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.1",
    "progress-bar-webpack-plugin": "^1.9.0",
    "sass-loader": "^4.0.2",
    "sassbean": "^0.1.1",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "0.0.31",
    "svgo-loader": "^1.1.1",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.3",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^8.5.3",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.2",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "vue": "^1.0.28",
    "vue-resource": "^1.0.3",
    "vue-router": "^0.7.13",
    "vuex": "^1.0.0"
  }
}
